        </rule>
       
        <rule name="Rewrite to ShowProductUR.aspx">
          <match url="^Shop/([0-9]+)/(.*?).htm" />
          <action type="Rewrite" url="Shop/ShowProductUR.aspx?Code={R:1}" />
        </rule>

        
        <rule name="Rewrite to URNewsByKeyword.aspx">
          <match url="^NK([0-9]+)_(.*?).html" />
          <action type="Rewrite" url="News/URNewsByKeyword.aspx?Code={R:1}" />
        </rule>

        <rule name="Rewrite to URNewsByKeywordName.aspx">
