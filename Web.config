          <action type="Rewrite" url="News/KeywordPics.aspx?Keyword={R:1}" />
        </rule>


        <rule name="Rewrite to URShowExternalNews.aspx">
          <match url="^N([0-9]+)_(.*?).html" />
          <action type="Rewrite" url="News/URShowExternalNews.aspx?Code={R:1}" />
        </rule>
        
        <rule name="Rewrite to URNewsByResource.aspx">
          <match url="^NR([0-9]+)_(.*?).html" />
          <action type="Rewrite" url="News/URNewsByResource.aspx?Code={R:1}" />
        </rule>
        <rule name="Rewrite to ShowNews.aspx">
          <match url="^News/([0-9]+).html" />
          <action type="Rewrite" url="News/ShowNews.aspx?Code={R:1}" />
        </rule>


        <rule name="Rewrite to World">
          <match url="^World$" />
